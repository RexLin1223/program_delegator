  "template": {
    "actions": [
      {
        "id": 1,
        "pre_action": 101,
        "name": "wpr_cpu_mem_start_record",
        "executable": "wpr.exe",
        "arguments": [
          {
            "command": "-start",
            "value": "Heap"
          },
          {
            "command": "-start",
            "value": "VirtualAllocation"
          },
          {
            "command": "-fileMode"
          }
        ],
        "post_action": 102,
        "property": 1,
        "output": "wpr_record.log"
      },
      {
        "id": 101,
        "name": "wpr_cancel_record",
        "executable": "wpr.exe",
        "arguments": [
          {
            "command": "-cancel"
          }
        ],
        "output": "wpr_cancel.log"
      },
      {
        "id": 102,
        "name": "wpr_stop_record",
        "executable": "wpr.exe",
        "arguments": [
          {
            "command": "-stop",
            "value": "{log_dir}\\wpr.etl"
          }
        ],
        "output": "wpr_stop.log"
      },
      {
        "id": 2,
        "name": "disk_vol_scan",
        "executable": "toolDiskVolScan.exe",
        "output": "disk_vol_scan.log"
      },
      {
        "id": 3,
        "name": "dsa_get_metrics",
        "executable": "sendCommand.cmd",
        "arguments": [
          {
            "command": "--get",
            "value": "Metrics"
          }
        ]
      },
      {
        "id": 4,
        "name": "dsa_diagnostic_package",
        "executable": "",
        "arguments": [
          {
            "command": "-g"
          }
        ]
      },
      {
        "id": 5,
        "name": "ratt",
        "executable": "ratt.exe",
        "arguments": [
          {
            "command": "-s",
            "value": "trace"
          },
          {
            "command": "-s",
            "value": "3"
          }
        ],
        "output": "ratt.log"
      },
      {
        "id": 6,
        "name": "enable_trace_log",
        "executable": "dsa_control.cmd",
        "arguments": [
          {
            "command": "--trace",
            "value": "9"
          }
        ],
        "output": "enable_trace.log"
      }
    ],
    "action_properties": [
      {
        "id": 0,
        "timeout_sec": 600
      },
      {
        "id": 1,
        "timeout_sec": 600,
        "period_sec": 60,
        "repeat": {
